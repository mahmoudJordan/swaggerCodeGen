{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/index.js", // Replace with the path to your main script
            "args": [
                "--output=\"./.generated/typescript-angular-client-generated\"",
                "--codeType=\"CLIENT\"",
                "--targetLanguage=\"typescript-angular\"",
                "--swaggerJsonUrl=\"https://stakeholders-api.dev.beaconconnect.app/swagger/v1/swagger.json\""
            ]
        }
    ]
}
